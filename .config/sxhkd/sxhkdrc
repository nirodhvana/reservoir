# Launch Firefox
super + shift + b
	firefox

# Launch Terminal file manager
super + shift + f
     st -e "so ~/.bashrc && /home/Saatwat/go/bin/lf"

# Launch GUI file manager
super + alt + f
     pcmanfm

# Launch Youtube-scraper in cli
super + shift + y
     st -e "ytfzf"

# Lauch ani-cli
super + shift + a
     st -e "ani-cli"

# Launch mangal
super + shift + m
     st -e "mangal"

# Shut down
super + shift + @Delete : Return
     st -e "sudo shutdown -h now"

# Reboot
super + shift + ctrl + @Delete : Return
     st -e "reboot"

